{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#292929",
          "foreground": "#46C1E0",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \ue0b1",
          "type": "os"
        },
        {
          "background": "#292929",
          "foreground": "#46C1E0",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ if .SSHSession }}\ueba9{{ end }}{{ .UserName }} <#46C1E0>\ue0b1</> ",
          "type": "session"
        },
        {
          "background": "#292929",
          "foreground": "#26C6DA",
          "style": "plain",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "background": "#292929",
          "foreground": "#46C1E0",
          "properties": {
            "style": "full",
            "trailing_diamond": "\ue0c6"
          },
          "style": "diamond",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#292929",
          "foreground": "#e2e2e2",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#292929",
          "background_templates": [
            "{{ if gt .Code 0 }}#292929{{ end }}"
          ],
          "foreground": "#46C1E0",
          "properties": {
            "always_enabled": true,
            "display_exit_code": true
          },
          "style": "diamond",
          "template": "{{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf00c{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "background": "#292929",
          "foreground": "#46C1E0",
          "properties": {
            "time_format": "15:04:05, _2"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} \uf073 ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}